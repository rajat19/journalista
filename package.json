{
  "name": "@rajat19/journalista",
  "version": "1.0.0",
  "description": "Personal journal cli app",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node index.ts",
    "start:watch": "nodemon --watch src --exec ts-node src/index.ts",
    "lint": "eslint --fix .",
    "build": "tsc && shx rm -rf dist/templates && shx cp -r templates dist",
    "renpack": "ts-node rename-package.ts"
  },
  "engines": {
    "node": ">=8.17.0"
  },
  "bin": {
    "jsniper": "./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajat19/journalista.git"
  },
  "author": "Rajat Srivastava",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^3.0.8",
    "inquirer": "^7.3.3",
    "moment": "^2.29.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/ejs": "^3.0.4",
    "@types/inquirer": "^7.3.1",
    "@types/node": "^14.10.3",
    "eslint": "^5.9.0",
    "jshint": "^2.9.5",
    "nodemon": "^2.0.4",
    "shx": "^0.3.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  }
}
